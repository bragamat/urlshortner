<h1>Reduzir urls</h1>

<%= form_for @changeset, Routes.url_path(@conn, :create), fn f -> %>
  <div class="container">
    <%= text_input f, :url, placeholder: "Cole a url que deseja shornted" %>
    <%= error_tag f, :url %>
  
  <%= submit "Reduzir" %>
  </div>
<% end %>

<p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>